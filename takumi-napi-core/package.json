{
  "name": "@takumi-rs/core",
  "version": "0.55.4",
  "author": {
    "email": "me@kane.tw",
    "name": "Kane Wang",
    "url": "https://kane.tw"
  },
  "keywords": [
    "image",
    "rendering",
    "css",
    "react",
    "jsx",
    "nodejs",
    "napi"
  ],
  "repository": {
    "url": "git+https://github.com/kane50613/takumi.git"
  },
  "main": "index.js",
  "devDependencies": {
    "@napi-rs/cli": "3.4.1",
    "@takumi-rs/helpers": "workspace:*",
    "@takumi-rs/template": "workspace:*",
    "@types/bun": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "^19.2.3",
    "lucide-react": "catalog:",
    "mitata": "^1.0.34",
    "react": "^19.2.0",
    "typescript": "catalog:"
  },
  "browser": "browser.js",
  "engines": {
    "node": ">= 12.22.0 < 13 || >= 14.17.0 < 15 || >= 15.12.0 < 16 || >= 16.0.0"
  },
  "license": "(MIT OR Apache-2.0)",
  "napi": {
    "binaryName": "core",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "aarch64-unknown-linux-gnu",
      "aarch64-unknown-linux-musl",
      "aarch64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "x86_64-pc-windows-msvc"
    ],
    "dtsHeaderFile": "./src/dts-header.d.ts"
  },
  "scripts": {
    "build": "napi build --release --no-const-enum --platform --esm --strip",
    "build:debug": "napi build",
    "prepublishOnly": "bun artifacts && cp artifacts/bindings-x86_64-unknown-linux-gnu/index.* . && napi prepublish -t npm --no-gh-release",
    "artifacts": "napi create-npm-dirs && napi artifacts && napi version",
    "bench": "bun tests/bench/index.tsx"
  },
  "type": "module",
  "types": "index.d.ts"
}
