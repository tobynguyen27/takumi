{
  "name": "@takumi-rs/image-response",
  "type": "module",
  "version": "0.62.1",
  "author": {
    "email": "me@kane.tw",
    "name": "Kane Wang",
    "url": "https://kane.tw"
  },
  "repository": {
    "url": "git+https://github.com/kane50613/takumi.git"
  },
  "dependencies": {
    "@takumi-rs/core": "workspace:*",
    "@takumi-rs/helpers": "workspace:*",
    "@takumi-rs/wasm": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsup": "^8.5.1",
    "type-fest": "^5.3.1",
    "typescript": "catalog:"
  },
  "scripts": {
    "build": "tsup-node src/backends/*.ts --clean --minify --dts --format esm,cjs --no-splitting",
    "prepublishOnly": "jq '.dependencies[\"@takumi-rs/core\"] = .version | .dependencies[\"@takumi-rs/helpers\"] = .version | .dependencies[\"@takumi-rs/wasm\"] = .version' package.json > tmp.json && mv tmp.json package.json"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js",
      "require": "./dist/node.cjs",
      "default": "./dist/node.js"
    },
    "./wasm": {
      "types": "./dist/wasm.d.ts",
      "import": "./dist/wasm.js",
      "require": "./dist/wasm.cjs",
      "default": "./dist/wasm.js"
    }
  },
  "license": "(MIT OR Apache-2.0)"
}
