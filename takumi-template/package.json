  },
  "devDependencies": {
    "@takumi-rs/core": "workspace:*",
    "@takumi-rs/helpers": "workspace:*",
    "@types/bun": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tsup": "^8.5.1",
    "typescript": "catalog:"
  },
  "scripts": {
    "build": "tsup-node src/templates/*.tsx --minify --dts --format esm,cjs --no-splitting"
